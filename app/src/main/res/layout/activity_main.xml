<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.yourorg.sample.MainActivity">

    <Button
        android:id="@+id/btVersions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Connect"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <WebView android:layout_width="match_parent" android:layout_height="match_parent" android:id="@+id/webview" android:usesCleartextTraffic="true" />
    <!--<VideoView android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/VideoViewID"
               tools:ignore="MissingConstraints"/>-->

</android.support.constraint.ConstraintLayout>
